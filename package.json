{
  "name": "evently-serverless",
  "version": "1.0.0",
  "description": "Serverless back end to publish and book events",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix --ext .js",
    "style:all": "npm run style:lint && npm run style:prettier",
    "style:lint": "eslint '**/*.js'",
    "style:prettier": "prettier --check '**/*.js'",
    "pretty:fix": "prettier --write '**/*.js'",
    "offline": "sls offline start --noAuth ",
    "deploy:dev": "sls deploy -s dev",
    "deploy:prod": "sls deploy -s prod",
    "export-env-local": "sls export-env --all",
    "e2e:test": "npm run e2e-create-item:test && npm run e2e-get-item:test && npm run e2e-get-item-by:test && npm run e2e-delete-item:test",
    "e2e-create-item:test": "jest --config config/end2end/e2e.create-item.jest.config.js",
    "e2e-delete-item:test": "jest --config config/end2end/e2e.delete-item.jest.config.js",
    "e2e-get-item:test": "jest --config config/end2end/e2e.get-item.jest.config.js",
    "e2e-get-item-by:test": "jest --config config/end2end/e2e.get-item-by.jest.config.js",
    "int:test": "jest --config config/integration/int.jest.config.js",
    "int:test-local": "npm run export-env-local && jest --config config/integration/int.jest.config.js",
    "test": "npm run int:test && npm run e2e:test"
  },
  "keywords": [],
  "author": "Francisco Javier Roca",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.312.0",
    "@aws-sdk/lib-dynamodb": "^3.312.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.6.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-dev-server": "^10.0.0",
    "jest-dynalite": "^3.6.1",
    "kill-port": "^2.0.1",
    "prettier": "^3.3.2",
    "serverless": "^3.32.2",
    "serverless-dynamodb": "^0.2.53",
    "serverless-export-env": "^2.2.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^13.6.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.359.0",
    "@aws-sdk/client-s3": "^3.342.0",
    "@middy/core": "^4.3.0",
    "@middy/http-error-handler": "^4.3.0",
    "@middy/http-header-normalizer": "^4.5.2",
    "@middy/http-json-body-parser": "^4.3.0",
    "@middy/http-multipart-body-parser": "^4.7.0",
    "@smithy/node-http-handler": "^3.1.1",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "ksuid": "^3.0.0",
    "superagent": "^9.0.2"
  }
}
